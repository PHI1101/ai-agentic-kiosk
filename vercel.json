{
  "version": 2,
  "functions": {
    "api/process-command.js": {
      "runtime": "@vercel/node"
    }
  },
  "builds": [
    {
      "src": "frontend/app",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/app/build/index.html"
    }
  ]
}
