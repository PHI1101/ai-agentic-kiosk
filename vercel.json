{
  "version": 2,
  "builds": [
    {
      "src": "frontend/app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/process-command.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/process-command",
      "dest": "api/process-command.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/app/build/index.html"
    }
  ]
}